{
    "name": "developeritsme/fiscal-service",
    "description": "Montenegro Fiscal Service",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Albert Suntic",
            "email": "albert@developer.its.me"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-xmlwriter": "*",
        "ext-dom": "*",
        "ext-openssl": "*",
        "ext-curl": "*",
        "nesbot/carbon": "^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "sempro/phpunit-pretty-print": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.75"
    },
    "autoload": {
        "psr-4": {
            "DeveloperItsMe\\FiscalService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
